<?xml version="1.0" encoding="UTF-8"?>
<server port="8282">
	<group id="lights">
		<field id="redLED">
			<title>Red LED</title>
			<integer>
				<minInclusive>0</minInclusive>
				<value>0</value>
			</integer>
			<tagName>LEDS!</tagName>
			<description>Red LED light for indicating a failure or off state.
			</description>
		</field>
		<field id="greenLED">
			<title>Green LED</title>
			<integer>
				<minInclusive>0</minInclusive>
				<value>1</value>
			</integer>
			<tagName>LEDS!</tagName>
			<description>Green LED light for indicating a failure or off state.
			</description>
		</field>
		<field id="blueLED">
			<title>Blue LED</title>
			<integer>
				<minInclusive>0</minInclusive>
				<value>100</value>
			</integer>
			<tagName>LEDS!</tagName>
			<description>Blue LED light for indicating a failure or off state.
			</description>
		</field>
	</group>
	<group id="switches">
		<field id="superswitch">
			<title>Super Switch</title>
			<integer>
				<minInclusive>0</minInclusive>
				<value>99</value>
			</integer>
			<tagName>SWITCHES!</tagName>
			<description>Some kinda switch.</description>
		</field>
	</group>
</server>
</xml>